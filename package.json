{
  "name": "mui-hook-form-control",
  "version": "0.0.1",
  "description": "Maretial UI react-hook-form control",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyemane/mui-hook-form-control.git"
  },
  "keywords": [
    "react",
    "Maretial",
    "UI",
    "react-hook-form"
  ],
  "author": "Dino Yemane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dyemane/mui-hook-form-control/issues"
  },
  "homepage": "https://github.com/dyemane/mui-hook-form-control#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "react": "16.14.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.9.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^27.0.4",
    "lodash": "^4.17.21",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
